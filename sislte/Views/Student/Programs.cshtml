<div class="app-content">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1>Programs</h1>
      </div>
    </div>
    <!--begin::Row-->
    <div class="row">
      <div class="card">
        <!-- /.card-header -->
        <div class="card-body table-responsive p-0">
          <table class="table table-hover text-nowrap">
            <thead>
            <tr>
              <th>Program Code</th>
              <th>Program Name</th>
              <th>Language</th>
            </tr>
            </thead>
            <tbody>
            @foreach (var program in @ViewBag.Programs)
            {
              <tr>
                <td>@program.Code</td>
                <td>@program.Name</td>
                <td>@program.Lang</td>
              </tr>
            }
            </tbody>
          </table>
        </div>
      </div>
      <!-- /.card-body -->
    </div>
  </div>
</div>